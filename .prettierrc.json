{
    "singleQuote": true,
    "printWidth": 150,
    "bracketSameLine": true,
    "plugins": ["@prettier/plugin-php"],
    "overrides": [
        {
            "files": ["*.php"],
            "options": {
                "parser": "php",
                "braceStyle": "1tbs",
                "trailingCommaPHP": false
            }
        }
    ]
}
